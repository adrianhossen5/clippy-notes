generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native"]
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id   String @id @unique @default(cuid())
  note Note?
}

model Note {
  id     String @id @unique @default(cuid())
  user   User   @relation(fields: [userId], references: [id])
  userId String @unique
}
